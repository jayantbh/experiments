{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single"],
      "interface-name": [false],
      "jsx-no-multiline-js": [false],
			"indent": [true, "tabs", 2],
			"object-literal-sort-keys": false,
			"curly": false,
			"no-empty": false
    },
    "rulesDirectory": []
}
